<!--pages/teacher/qiandao/qiandao.wxml-->
<view class="page">
  <view class="page__bd">
    <view class="weui-cells__title">学生签到信息</view>
    <block wx:for="{{list}}" wx:key="{{index}}">
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell">
          <view class="weui-cell__bd">
            <view>{{list[index].courseobj.name}}</view>
            <view>{{list[index].studentobj.name}} {{status[index]}}</view>
          </view>
          <view class="weui-cell__ft">
            <view>开始时间：{{satarttime[index]}}</view>
            <view>结束时间：{{endtime[index]}}</view>
          </view>
        </view>
      </view>
    </block>
  </view>
</view>