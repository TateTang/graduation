/*

 tabbar 图标wxss样式
*/
.tab-bar{
  position: fixed;
  bottom:0;
  left:0;
  width:100%;
  background-color: #fff;
  border-top:1px solid #E0E0E0;
  z-index:500;
}
.tab-bar .tab-item{
  float:left;
  width: 33%;
  text-align: center;
  padding-top:5px;
}

.tab-bar .tab-item .icons{
  font-size:22px;
  color:#808080;
  font-weight:800;
}
.tab-bar .tab-item .icon-text{
  font-size:10px;
  display: block;
  line-height:2.2;
  color:#808080;
}
.tab-bar .active .icons{
  color:#1296DB;

}
.tab-bar .active .icon-text{
  color:#1296DB;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}


.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-ziyuan:before {
  content: "\e621";
}

.icon-gongzuotai:before {
  content: "\e68c";
}

.icon-qiandao:before {
  content: "\e61f";
}